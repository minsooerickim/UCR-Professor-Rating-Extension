{
    "manifest_version": 2,
    "name": "Testing",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": [
                "https://registrationssb.ucr.edu/*",
                "http://registrationssb.ucr.edu/*"
            ],
            "js": [
                "jquery-3.5.1.js", 
                "content.js",
                "background.js"
            ]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "rating.png"
    },
    "permissions": [
        "nativeMessaging"
    ]
}